{
  "expo": {
    "name": "Personal Compass",
    "slug": "personal-compass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anthonyvnguyen.personalcompass",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location to show your position on the map and calculate compass directions.",
          "locationAlwaysPermission": "This app needs access to location to show your position on the map and calculate compass directions.",
          "locationWhenInUsePermission": "This app needs access to location to show your position on the map and calculate compass directions."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f0773612-e1ff-4232-ba97-994142a714fb"
      }
    }
  }
}
